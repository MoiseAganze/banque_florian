{
  "QtBinding": "PySide6",
  "ShowLogs": true,
  "LiveCompileQss": false,
  "CheckForMissingicons": false,
  "QDialog": [
    {
      "tittle": "Authentification"
    }
  ],
  "QSettings": [
    {
      "ThemeSettings": [
        {
          "CustomTheme": [
            {
              "Background-color": "#FFFFFF",
              "Text-color": "#010000",
              "Icons-color": "#000000",
              "Accent-color": "#26bae3",
              "Theme-name": "LightBlue",
              "Default-Theme": true
            },
            {
              "Background-color": "#21272a",
              "Text-color": "#fefefe",
              "Icons-color": "#fefefe",
              "Accent-color": "#fba43b",
              "Theme-name": "DarkYellow",
              "Default-Theme": false
            }
          ]
        }
      ]
    }
  ],
  "QCustomQStackedWidget": [
    {
      "name": "auth_pages",
      "navigation": [
        {
          "navigationButtons": [
            {
              "link_connexion": "page_connexion",
              "link_inscription": "page_inscription"
            }
          ]
        }
      ],
      "transitionAnimation": [
        {
          "slide": [
            {
              "active": true,
              "duration": 500,
              "easingCurve": "InSine",
              "direction": "horizontal"
            }
          ]
        }
      ]
    }
  ]
}
